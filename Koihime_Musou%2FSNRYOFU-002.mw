<String index="3">
 <Origin>SE062</Origin>
 <ToTranslate>SE062</ToTranslate>
 </String>
<String index="4">
 <Origin>tcg_kan11ca</Origin>
 <ToTranslate>tcg_kan11ca</ToTranslate>
 </String>
<String index="5">
 <Origin>tcg_kan11m</Origin>
