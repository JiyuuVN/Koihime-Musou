  <String index="254">
    <Origin>syot0015</Origin>
    <ToTranslate>syot0015</ToTranslate>
  </String>
  <String index="255">
    <Origin>Girl$"I knew it! So you are the Messenger from Heaven, right?"@</Origin>
    <ToTranslate>Девочка$"Я так и думала! Значит вы Посланник Небес, верно?"@</ToTranslate>
  </String>
  <String index="256">
    <Origin>Kazuto$"Yes, they do call me that. ......But what about it?"@</Origin>
    <ToTranslate>Казуто$"Да, иногда меня так называют. ......Но зачем тебе это?"@</ToTranslate>
  </String>
  <String index="257">
    <Origin>tcg_syo22ma</Origin>
    <ToTranslate>tcg_syo22ma</ToTranslate>
  </String>
