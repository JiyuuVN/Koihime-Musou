  <String index="148">
    <Origin>Girl $"Hawawa, hawawa, hawawa, hawawa...!"@</Origin>
    <ToTranslate>Девочка$"Хавава, хавава, хавава, хавава...!"@</ToTranslate>
  </String>
  <String index="149">
    <Origin>Old Woman$"Hah, hah, hah, hah..."@</Origin>
    <ToTranslate>Пожилая женщина$"Фух,фух, фух, фух..."@</ToTranslate>
  </String>
  <String index="150">
    <Origin>syot0001</Origin>
    <ToTranslate>syot0001</ToTranslate>
  </String>
  <String index="151">
    <Origin>Girl$"Come on, just a little more, ma&apos;am. Hang in there!!"@</Origin>
    <ToTranslate>Девочка$"Давайте, ещё чуть-чуть, бабушка. Идёмте!!"@</ToTranslate>
  </String>
  <String index="152">
    <Origin>Old Woman$"I&apos;m already done for......Please save yourself, dear."@</Origin>
    <ToTranslate>Пожилая женщина$"Я слишком стара для этого......пожалуйста, уходи сама."@</ToTranslate>
  </String>
  <String index="153">
    <Origin>tcg_syo22ta</Origin>
    <ToTranslate>tcg_syo22ta</ToTranslate>
  </String>
  <String index="154">
