    <ToTranslate>tcg_saj31aa</ToTranslate>
  </String>
  <String index="323">
    <ToTranslate>tcg_saj31m</ToTranslate>
  </String>
  <String index="324">
    <ToTranslate>_MsgBoxMain_On</ToTranslate>
  </String>
  <String index="325">
    <ToTranslate>saji0117</ToTranslate>
  </String>
  <String index="326">
    <ToTranslate>Саджи$"Ра-а-а!"@</ToTranslate>
  </String>
  <String index="327">
    <ToTranslate>battle_saj3</ToTranslate>
  </String>
