  <String index="415">
    <Origin>Kazuto$"Hey, what are you doing?"@</Origin>
    <ToTranslate>Казуто$"Эй, ты что делаешь?"@</ToTranslate>
  </String>
  <String index="416">
    <Origin>saji0007</Origin>
    <ToTranslate>saji0007</ToTranslate>
  </String>
  <String index="417">
    <Origin>Male student$"You&apos;re in my way."@</Origin>
    <ToTranslate>Ученик$"Уйди с дороги."@</ToTranslate>
  </String>
  <String index="418">
    <Origin>battle_keri</Origin>
    <ToTranslate>battle_keri</ToTranslate>
  </String>
  <String index="419">
    <Origin>Kazuto$"Whoa! W-Weren&apos;t you listening to me?!"@</Origin>
    <ToTranslate>Казуто$"Т-ты что, меня не слушал?!"@</ToTranslate>
  </String>
  <String index="420">
    <Origin>tcg_saj12gc</Origin>
    <ToTranslate>tcg_saj12gc</ToTranslate>
  </String>
  <String index="421">
